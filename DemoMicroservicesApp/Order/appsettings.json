{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CapConfig": {
    "FailedRetryIntervalSeconds": 90,
    "FailedRetryCount": 10,
    "SucceedMessageExpirationSeconds": 86400,
    "FailedMessageExpirationSeconds": 864000
  },
  "RabbitMqConfig": {
    "ExchangeName": "order-created-topic-exchange"
  }
}
